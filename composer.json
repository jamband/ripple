{
  "name": "jamband/ripple",
  "description": "Get a track/album information from the URL.",
  "keywords": ["oembed", "embed", "scraping", "youtube", "vimeo", "soundcloud", "bandcamp"],
  "homepage": "https://github.com/jamband/ripple",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Tomoki Morita",
      "email": "tmsongbooks215@gmail.com",
      "homepage": "https://jamband.github.io/"
    }
  ],
  "require": {
    "php": "^8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "phpstan/phpstan": "^1.9",
    "friendsofphp/php-cs-fixer": "^3.13"
  },
  "autoload": {
    "psr-4": {
      "Jamband\\Ripple\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "scripts": {
    "format": "php-cs-fixer fix --dry-run",
    "analyse": "phpstan analyse --no-progress --memory-limit=2G --xdebug"
  }
}
